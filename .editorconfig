# http://editorconfig.org
root = true

[*]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.json]
indent_style = tab

[*.cdc]
indent_style = space
indent_size = 4

[*.md]
trim_trailing_whitespace = false

[*.pkey]
insert_final_newline = false

[*.key]
insert_final_newline = false
